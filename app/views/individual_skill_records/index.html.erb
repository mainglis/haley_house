<h1>Listing individual_skill_records</h1>

<table>
  <tr>
    <th>Individual</th>
    <th>Skill</th>
    <th>Date gained</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @individual_skill_records.each do |individual_skill_record| %>
  <tr>
    <td><%= individual_skill_record.individual_id %></td>
    <td><%= individual_skill_record.skill_id %></td>
    <td><%= individual_skill_record.date_gained %></td>
    <td><%= link_to 'Show', individual_skill_record %></td>
    <td><%= link_to 'Edit', edit_individual_skill_record_path(individual_skill_record) %></td>
    <td><%= link_to 'Destroy', individual_skill_record, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

